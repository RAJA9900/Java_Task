package regex;

public class Dl_check {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(isValidDl("TN14 2018 0000134"));
		System.out.println(isValidDl("TN1420180000134"));
		System.out.println(isValidDl("TN142018000ABv134"));
		

	}
	
	public static boolean isValidDl(String num)
	{
		return num!=null && num.matches("[A-Z]{2}[0-9]{13}");
	}	
}